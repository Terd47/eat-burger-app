
<h1>We Love Our burgers and we Guarantee you will too</h1>

<div class="new">
  <h2>Fresh New Burgers</h2>
  <ol class="draggable">
  {{#each burger}}
    {{#unless devoured}}
        <li id={{id}} >{{burger_name}}</li> 
    {{/unless}}
  {{/each}}
</ol>
</div>

<div  id="devoured">
  <h2>Devoured Burgers</h2>
<ol class="droppable" >
  {{#each burger}}
    {{#if devoured}}
     <li id={{id}} >{{burger_name}}</li> 
    {{/if}}
  {{/each}}
</ol>
</div>
<form class="create-form">

  <div class="form-group">
    <label for="ca"></label>
    <input type="text" id="ca" name="name">
  </div>

  <button type="submit" id="sub">Submit</button>
</form>
<div class="container" id="notice">
  <p>To devour, drag new burger to devoured burger</p>
  <hr>
</div>

